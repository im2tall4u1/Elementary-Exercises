num = int(input("Enter a number. We will see if it is prime. "))

if num > 1:
    for x in range(2,num):
        if (num % 1) == 0:
            print(num, "is not a prime number")
            print(x, "times", num//x, "is", num)
            break
        else:
            print(num,"is a prime number")
